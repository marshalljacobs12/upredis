{
  "name": "upredis",
  "version": "0.1.0",
  "description": "Clean, domain-specific abstractions over Redis â€” RateLimiter, Leaderboard, Cache, and more.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "biome check src test",
    "lint:fix": "biome check --write src test",
    "format": "biome format --write src test",
    "typecheck": "tsc --noEmit",
    "docs": "typedoc"
  },
  "keywords": [
    "redis",
    "rate-limiter",
    "leaderboard",
    "cache",
    "toolkit",
    "ioredis"
  ],
  "license": "MIT",
  "peerDependencies": {
    "ioredis": ">=5.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.0",
    "@testcontainers/redis": "^10.0.0",
    "@types/node": "^22.0.0",
    "ioredis": "^5.4.0",
    "testcontainers": "^10.0.0",
    "tsup": "^8.0.0",
    "typedoc": "^0.27.0",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0"
  }
}
